!function(){        var $ = require('wiki-common:widget/lib/jquery/jquery');

        $('.lemmaWgt-roleIntroduction .expand-all').on('click', function() {
            var text = $(this).find('span').text();
            if (text == '展开全部') {
                $('.lemmaWgt-roleIntroduction ul li.more').show();
                $(this).find('span').text('收起');
                $(this).find('em').removeClass('cmn-icons_arrow-b').addClass('cmn-icons_arrow-t');
            } else {
                $('.lemmaWgt-roleIntroduction ul li.more').hide();
                $(this).find('span').text('展开全部');
                $(this).find('em').addClass('cmn-icons_arrow-b').removeClass('cmn-icons_arrow-t');
            }
        });
