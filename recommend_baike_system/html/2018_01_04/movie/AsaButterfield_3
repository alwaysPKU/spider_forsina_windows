    var videoSourceList = [{"source":"iqiyi","isAuto":true,"isHide":false,"mediaId":"166479700"},{"source":"hunantv","isAuto":true,"isHide":false,"mediaId":"45843"},{"source":"youku","isAuto":true,"isHide":false,"mediaId":"2de9a23abcb811e0bf93"},{"source":"tencent","isAuto":true,"isHide":false,"mediaId":"pgsr8fnw41jsght"},{"source":"pptv","isAuto":true,"isHide":false,"mediaId":"24771633"},{"source":"kankan","isAuto":true,"isHide":false,"mediaId":"71330"},{"source":"letv","isAuto":true,"isHide":false,"mediaId":"92357"}];
    for (var i in videoSourceList) {
