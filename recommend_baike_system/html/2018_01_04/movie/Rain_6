!function(){    var $ = require('wiki-common:widget/lib/jquery/jquery');
    
    $('#staffList').on('click', '.more a', function(){
        var btn = $(this),
            staffList = $('#staffList');

        if (!staffList.hasClass('fold')) {
            btn.text('展开');
            staffList.addClass('fold');
        } else {
            btn.text('收起');
            staffList.removeClass('fold');
        }
    });
}();
