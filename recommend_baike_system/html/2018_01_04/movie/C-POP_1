        },
        error: function () {
            // log 词条页广告被拦截情况（此处 log 请求失败）
            nslog(70000101, window.location.href, {
                api: 'lemma-ad',
                action: 'error'
            });
        }
    });

    // 设置分享内容
    window.BKShare.setCommon({
        bdText: "\u3010\u8377\u5858\u6708\u8272_\u767e\u5ea6\u767e\u79d1\u3011\u300a\u8377\u5858\u6708\u8272\u300b\u662f\u51e4\u51f0\u4f20\u5947\u6f14\u5531\u7684\u4e00\u9996\u4e2d\u56fd\u98ce\u6b4c\u66f2\uff0c\u7531\u5f20\u8d85\u4f5c\u66f2\u586b\u8bcd\uff0c\u4e0e\u300a\u7231\u4f60\u7231\u5230\u6389\u4e86\u7259\u300b\u540c\u66f2\u5f02\u8bcd\u3002\u6536\u5f55\u5165\u4e13\u8f91\u300a\u6211\u4ece\u8349\u539f\u6765\u300b\u3002.....\uff08\u5206\u4eab\u81ea@\u767e\u5ea6\u767e\u79d1\uff09",
        bdDesc: '',
        bdUrl: 'http:\/\/baike.baidu.com\/item\/%E8%8D%B7%E5%A1%98%E6%9C%88%E8%89%B2\/14158',
        bdPic: '',
        onBeforeClick: function (){
            $('.bdshare_dialog_box').hide();
        }
    });

    // 底部投诉带入当前页面 url
    var map = [1, 2, 6, 5];
    $('.wgt-footer-main .suggestion').find('a').each(function(i) {
        $(this).attr('href', 'http://help.baidu.com/newadd?word=%E8%8D%B7%E5%A1%98%E6%9C%88%E8%89%B2' + '&&submit_link=' + encodeURIComponent(window.location.href) + '&prod_id=10&category=' + map[i]);
    });

    // 为超出预设内容的表格添加table-responsive控制
    $('.main-content').find('table').each(function(index) {
        var $that = $(this);
        if ($that.width() > 790) {
            $that.wrap('<div class="table-responsive"></div>');
        }
    });
