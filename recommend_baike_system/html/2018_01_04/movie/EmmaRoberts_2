!function(){    var nslog = require('wiki-common:widget/component/nslog/nslog'),
        init = require('wiki-lemma:widget/lemma_content/configModule/tab/video/video');
    
    !!window.ScriptLazyLoad && window.ScriptLazyLoad.regist({
        dom: $('.lemmaWgt-tab-video'),
        loadFunc: function() {
            init({"newLemmaId":9350478,"videoSourceList":{"list":[{"source":"iqiyi","isAuto":false,"isHide":true,"mediaId":"147200"},{"source":"youku","isAuto":true,"isHide":false,"mediaId":"cc06b37a962411de83b1"}]},"category":"movieLarge"});
        },
        distance: 0
    });

    /*****nslog统计采集 START*****/
    // 不同类型 tab 词条 pv 采集
    //  多少义项有视频tab页，这些义项的日均pv总和，细分到电影，电视剧，综艺分类
    switch ('movieLarge') {
        case 'movieSmall':
        case 'movieLarge':
            nslog(1001901);
            break;
        case 'dramaSmall':
            nslog(1001902);
            break;
        case 'varietySmall':
            nslog(1001903);
            break;
    }
    
// 分别统计多少义项的视频tab页有爱奇艺/乐视/迅雷看看的长视频资源，这些义项的日均pv总和，爱奇艺/乐视
// 迅雷看看的独家长视频资源分别是多少，这些义项的日均pv总和

    var videoSourceList = [{"source":"iqiyi","isAuto":false,"isHide":true,"mediaId":"147200"},{"source":"youku","isAuto":true,"isHide":false,"mediaId":"cc06b37a962411de83b1"}];
    for (var i in videoSourceList) {
        switch (videoSourceList[i].source) {
            case 'iqiyi':
                nslog(1001931);
                if (videoSourceList.length == 1) {
                    nslog(1001941);
                }
                break;
            case 'letv':
                nslog(1001932);
                if (videoSourceList.length == 1) {
                    nslog(1001942);
                }
                break;
