!function(){    var nslog = require('wiki-common:widget/component/nslog/nslog'),
        init = require('wiki-lemma:widget/lemma_content/configModule/tab/video/video');
    
    !!window.ScriptLazyLoad && window.ScriptLazyLoad.regist({
        dom: $('.lemmaWgt-tab-video'),
        loadFunc: function() {
            init({"newLemmaId":14125,"videoSourceList":{"list":[{"source":"hunantv","isAuto":true,"isHide":false,"mediaId":"1366"},{"source":"tencent","isAuto":true,"isHide":false,"mediaId":"533"}]},"category":"varietySmall"});
        },
        distance: 0
    });

    /*****nslog统计采集 START*****/
    // 不同类型 tab 词条 pv 采集
    //  多少义项有视频tab页，这些义项的日均pv总和，细分到电影，电视剧，综艺分类
    switch ('varietySmall') {
        case 'movieSmall':
        case 'movieLarge':
            nslog(1001901);
            break;
        case 'dramaSmall':
            nslog(1001902);
            break;
        case 'varietySmall':
            nslog(1001903);
            break;
    }
    
// 分别统计多少义项的视频tab页有爱奇艺/乐视/迅雷看看的长视频资源，这些义项的日均pv总和，爱奇艺/乐视
// 迅雷看看的独家长视频资源分别是多少，这些义项的日均pv总和

    var videoSourceList = [{"source":"hunantv","isAuto":true,"isHide":false,"mediaId":"1366"},{"source":"tencent","isAuto":true,"isHide":false,"mediaId":"533"}];
    for (var i in videoSourceList) {
        switch (videoSourceList[i].source) {
            case 'iqiyi':
                nslog(1001931);
                if (videoSourceList.length == 1) {
                    nslog(1001941);
                }
                break;
            case 'letv':
                nslog(1001932);
                if (videoSourceList.length == 1) {
                    nslog(1001942);
                }
                break;
            case 'kankan':
                nslog(1001933);
                if (videoSourceList.length == 1) {
                    nslog(1001943);
                }
                break;
            case 'youku':
                nslog(1001934);
                if (videoSourceList.length == 1) {
                    nslog(1001944);
                }
                break;
            case 'hunantv':
                nslog(1001935);
                if (videoSourceList.length == 1) {
                    nslog(1001945);
                }
                break;
            case 'tencent':
                nslog(1001936);
                if (videoSourceList.length == 1) {
                    nslog(1001946);
                }
                break;
            case 'pptv':
                nslog(1001937);
                if (videoSourceList.length == 1) {
                    nslog(1001947);
                }
                break;
            case 'm1905':
                nslog(1001938);
                if (videoSourceList.length == 1) {
                    nslog(1001948);
                }
                break;
        }
    }

    // 不同类型 tab 展现量采集（仅采集一次）
    // 视频tab的点击量，细分到电影，电视剧，综艺分类
    $('.tabCards .videoTab').on('click', function() {
        switch ('varietySmall') {
            case 'movieSmall':
            case 'movieLarge':
                nslog(1001911);
                break;
            case 'dramaSmall':
                nslog(1001912);
                break;
            case 'varietySmall':
                nslog(1001913);
                break;
        }
        $('.tabCards .videoTab').off('click');
    });
    /*****nslog统计采集 END*****/
