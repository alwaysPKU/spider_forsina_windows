                        if ('1') {
                            $('.lemmaWgt-lemmaTitle .add-subLemma').css('display', 'inline-block');
                            $('.top-tool .add-sub-icon').css('display', 'inline-block');
                        }
                    }
                });
                require('wiki-lemma:widget/basicElement/addSub/addSub.js')({                    lemmaId: '6142415',                    lemmaTitle: '奇迹MU',                    lemmaDesc: '',                    versionId: '121049235',                    subLemmaId: '1066038'                });
            }();
