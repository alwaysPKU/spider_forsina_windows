        $(this).attr('href', 'http://help.baidu.com/newadd?word=%E5%B0%91%E5%B9%B4%E9%AD%94%E6%B3%95%E5%B8%88' + '&&submit_link=' + encodeURIComponent(window.location.href) + '&prod_id=10&category=' + map[i]);
    });

    // 为超出预设内容的表格添加table-responsive控制
    $('.main-content').find('table').each(function(index) {
        var $that = $(this);
        if ($that.width() > 790) {
            $that.wrap('<div class="table-responsive"></div>');
        }
    });
