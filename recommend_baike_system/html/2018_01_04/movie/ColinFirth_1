                        if ('1') {
                            $('.lemmaWgt-lemmaTitle .add-subLemma').css('display', 'inline-block');
                            $('.top-tool .add-sub-icon').css('display', 'inline-block');
                        }
                    }
                });    
                require('wiki-lemma:widget/basicElement/addSub/addSub.js')({                    lemmaId: '4085447',                    lemmaTitle: '单身男子',                    lemmaDesc: '',                    versionId: '111951807',                    subLemmaId: '3021395'                });        
            }();
