                            $('.lemmaWgt-lemmaTitle .add-subLemma').css('display', 'inline-block');
                            $('.top-tool .add-sub-icon').css('display', 'inline-block');
                        }
                    }
                });    
                require('wiki-lemma:widget/basicElement/addSub/addSub.js')({                    lemmaId: '12005706',                    lemmaTitle: '星球大战2：克隆人的进攻',                    lemmaDesc: '',                    versionId: '120330569',                    subLemmaId: '11255640'                });        
