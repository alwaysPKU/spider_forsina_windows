        },
        distance: 0
    });

    /*****nslog统计采集 START*****/
    // 不同类型 tab 词条 pv 采集
    //  多少义项有视频tab页，这些义项的日均pv总和，细分到电影，电视剧，综艺分类
    switch ('movieLarge') {
        case 'movieSmall':
        case 'movieLarge':
            nslog(1001901);
            break;
        case 'dramaSmall':
            nslog(1001902);
            break;
        case 'varietySmall':
            nslog(1001903);
            break;
    }
    
// 分别统计多少义项的视频tab页有爱奇艺/乐视/迅雷看看的长视频资源，这些义项的日均pv总和，爱奇艺/乐视
// 迅雷看看的独家长视频资源分别是多少，这些义项的日均pv总和

    var videoSourceList = [{"source":"iqiyi","isAuto":true,"isHide":false,"mediaId":"237538500"},{"source":"hunantv","isAuto":true,"isHide":false,"mediaId":"290276"},{"source":"youku","isAuto":true,"isHide":false,"mediaId":"cc035b9e962411de83b1"}];
    for (var i in videoSourceList) {
        switch (videoSourceList[i].source) {
            case 'iqiyi':
                nslog(1001931);
                if (videoSourceList.length == 1) {
                    nslog(1001941);
                }
                break;
            case 'letv':
                nslog(1001932);
                if (videoSourceList.length == 1) {
                    nslog(1001942);
                }
                break;
            case 'kankan':
                nslog(1001933);
                if (videoSourceList.length == 1) {
                    nslog(1001943);
                }
                break;
            case 'youku':
                nslog(1001934);
                if (videoSourceList.length == 1) {
                    nslog(1001944);
                }
                break;
            case 'hunantv':
                nslog(1001935);
                if (videoSourceList.length == 1) {
                    nslog(1001945);
                }
                break;
            case 'tencent':
                nslog(1001936);
                if (videoSourceList.length == 1) {
                    nslog(1001946);
                }
                break;
            case 'pptv':
                nslog(1001937);
                if (videoSourceList.length == 1) {
                    nslog(1001947);
                }
                break;
            case 'm1905':
                nslog(1001938);
                if (videoSourceList.length == 1) {
                    nslog(1001948);
                }
                break;
        }
    }

    // 不同类型 tab 展现量采集（仅采集一次）
    // 视频tab的点击量，细分到电影，电视剧，综艺分类
    $('.tabCards .videoTab').on('click', function() {
