                nslog(1001933);
                if (videoSourceList.length == 1) {
                    nslog(1001943);
                }
                break;
            case 'youku':
                nslog(1001934);
                if (videoSourceList.length == 1) {
                    nslog(1001944);
                }
                break;
            case 'hunantv':
                nslog(1001935);
                if (videoSourceList.length == 1) {
                    nslog(1001945);
                }
                break;
