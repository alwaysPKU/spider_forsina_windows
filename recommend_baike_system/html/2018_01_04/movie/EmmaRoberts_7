        case 'dramaSmall':
            nslog(1001902);
            break;
        case 'varietySmall':
            nslog(1001903);
            break;
    }
    
// 分别统计多少义项的视频tab页有爱奇艺/乐视/迅雷看看的长视频资源，这些义项的日均pv总和，爱奇艺/乐视
// 迅雷看看的独家长视频资源分别是多少，这些义项的日均pv总和

    var videoSourceList = [{"source":"tencent","isAuto":true,"isHide":false,"mediaId":"tnabsm3cxc91i2p"},{"source":"letv","isAuto":true,"isHide":false,"mediaId":"92338"}];
    for (var i in videoSourceList) {
